{
  "extends": ["../../.eslintrc.cjs", "plugin:react/recommended", "plugin:react-hooks/recommended"],
  "parserOptions": {
    "project": ["./tsconfig.json", "./tsconfig.node.json"],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "react-hooks"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  }
}
